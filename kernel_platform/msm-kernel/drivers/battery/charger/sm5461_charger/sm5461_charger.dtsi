&sm5461_charger {
	sm5461,en_vbatreg = <1>;
	sm5461,single_mode = <1>;
	sm5461,vbus_ctrl_mode = <1>;
	sm5461,fpdo_topoff = <1500000>; /* 1500mA */
	sm5461,fpdo_vnow_reg = <5000000>; /* 5000mV */
	sm5461,fpdo_mainvbat_reg = <4237000>; /* 4237mV */
	sm5461,fpdo_subvbat_reg = <4203000>; /* 4203mV */
};