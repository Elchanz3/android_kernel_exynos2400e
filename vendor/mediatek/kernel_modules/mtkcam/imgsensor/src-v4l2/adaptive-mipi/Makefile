$(info start build of imgsensor_adaptive_mipi)

# Build common adaptive_mipi files
subdir-ccflags-y += \
	-I$(IMGSENSOR_DRIVER_PATH)/$(ADAPTIVE_MIPI)

ADAPTIVE_MIPI_OBJS += $(ADAPTIVE_MIPI)/imgsensor_adaptive_mipi.o

$(info ADAPTIVE_MIPI_OBJS : $(ADAPTIVE_MIPI_OBJS))
$(info build of imgsensor_adaptive_mipi completed)
