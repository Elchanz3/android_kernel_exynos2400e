/ {
	fragment@smd {
		target-path = "/";
			__overlay__ {
				samsung_mobile_device {
					dc_vibrator {
						compatible = "samsung,dc_vibrator";
						status = "okay";
						dc_vib,motor_type = "COINDC";
#if 1
						dc_vib,gpio_en = <SEC_GPIO_REF(AP,pio,110) 0>;
#endif
#if 0
						dc_vib,regulator_name = "dc_vib";
						dc_vib-supply = <&${motor_regul_vdd_3p3}>;
#endif
					};
				};
			};
	};
};
